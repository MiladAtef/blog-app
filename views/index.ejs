<!DOCTYPE html>
<html lang="en">
	<head>
		<% include partials/header %>
		<title>Blog App</title>
	</head>
	<body>
		<% include partials/navbar %>
		<div class="ui container main">
			<%blogs.forEach(({body,created,title,image})=>{%>
			<h2><%= title %></h2>
			<img src="<%= image %>" />
			<span><%= created.toLocaleString() %></span>
			<p><%= body %></p>
			<%})%>
		</div>
		<% include partials/footer %>
	</body>
</html>
